  MdeModulePkg/MdeModulePkg.dec
  MdePkg/MdePkg.dec
  Broadcom/BroadcomPkg.dec
  Pi5Pkg/Pi5Pkg.dec

[FixedPcd]
  gArmTokenSpaceGuid.PcdArmArchTimerIntrNum
  gArmTokenSpaceGuid.PcdArmArchTimerHypIntrNum
