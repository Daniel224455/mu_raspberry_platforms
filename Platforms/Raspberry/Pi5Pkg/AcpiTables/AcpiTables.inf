  Dsdt.asl
  Fadt.aslc
  Gtdt.aslc
  Madt.aslc
  Pptt.aslc
  Spcr.aslc
  Mcfg.aslc

[Packages]
  ArmPkg/ArmPkg.dec
  ArmPlatformPkg/ArmPlatformPkg.dec
